{
    "name":  "srr-tasks",
    "version":  "0.0.0",
    "scripts":  {
                    "ng":  "ng",
                    "start":  "ng serve",
                    "build":  "ng build",
                    "watch":  "ng build --watch --configuration development",
                    "test":  "vitest run",
                    "test:coverage":  "vitest run --coverage",
                    "test:vitest":  "vitest run",
                    "test:vitest:watch":  "vitest",
                    "test:e2e":  "start-server-and-test \"npm run start -- --host 127.0.0.1 --port 4200\" http://127.0.0.1:4200 \"cypress run\"",
                    "test:e2e:ui":  "cypress open",
                    "serve:ssr:srr-tasks":  "node dist/srr-tasks/server/server.mjs"
                },
    "prettier":  {
                     "overrides":  [
                                       {
                                           "files":  "*.html",
                                           "options":  {
                                                           "parser":  "angular"
                                                       }
                                       }
                                   ]
                 },
    "private":  true,
    "dependencies":  {
                         "@angular/animations":  "^20.3.16",
                         "@angular/cdk":  "^20.2.14",
                         "@angular/common":  "^20.3.16",
                         "@angular/compiler":  "^20.3.16",
                         "@angular/core":  "^20.3.16",
                         "@angular/forms":  "^20.3.16",
                         "@angular/platform-browser":  "^20.3.16",
                         "@angular/platform-server":  "^20.3.16",
                         "@angular/router":  "^20.3.16",
                         "@angular/ssr":  "^20.3.16",
                         "@ngx-translate/core":  "^17.0.0",
                         "@ngx-translate/http-loader":  "^17.0.0",
                         "@primeuix/themes":  "^2.0.2",
                         "express":  "^5.1.0",
                         "primeflex":  "^4.0.0",
                         "primeicons":  "^7.0.0",
                         "primeng":  "^20.0.0",
                         "rxjs":  "~7.8.0",
                         "tslib":  "^2.3.0",
                         "zone.js":  "~0.15.0"
                     },
    "devDependencies":  {
                            "@analogjs/vite-plugin-angular":  "^1.20.0",
                            "@analogjs/vitest-angular":  "^1.20.0",
                            "@angular/build":  "^20.3.16",
                            "@angular/cli":  "^20.3.16",
                            "@angular/compiler-cli":  "^20.3.16",
                            "@angular/platform-browser-dynamic":  "^20.3.16",
                            "@types/express":  "^5.0.1",
